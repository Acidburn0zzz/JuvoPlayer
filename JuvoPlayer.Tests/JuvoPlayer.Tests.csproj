    <None Remove="res\MPD Parser Data\TOS_OnDemand_HEVC_MultiRate.mpd" />
    <None Remove="res\MPD Parser Data\TOS_OnDemand_HEVC_MultiRes.mpd" />
    <None Remove="res\MPD Parser Data\xlink.xsd" />
  </ItemGroup>

