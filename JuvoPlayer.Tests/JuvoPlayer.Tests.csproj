<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netcoreapp2.0;netstandard2.0</TargetFrameworks>

    <IsPackable>false</IsPackable>

    <StartupObject></StartupObject>
